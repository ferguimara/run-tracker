<%- include('../partials/header') %>

<table id="list">
    <thead>
        <th></th>
        <th>Full Name:</th>
        <th>Age:</th>
        <th>Event:</th>
        <th></th>
    </thead>
    <tbody>
        <% users.forEach(user => {%>
            <tr>
                <td><form action="/users/<%= user._id %>?_method=DELETE" method="POST">
                    <button type="submit">‚ùå</button>
                </form></td>
                <td><%= user.name%></td>
                <td><%= user.age%></td>
                <td><%= user.event%></td>
                <td><a href="/users/<%=user._id%>">View Dashboard</a></td>
            </tr>
        <%})%>
    </tbody>
</table>

<%- include('../partials/footer') %>